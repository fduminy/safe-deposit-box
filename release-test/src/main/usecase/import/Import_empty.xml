<release-test name="Import_empty"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="http://test-release.xsd"
              enabled="true">

    <description>Check import of an empty csv file with and without headers</description>
    
    <call-method file="Import.xmf">
        <parameter name="importType" value="empty_no_headers"/>
        <parameter name="sheetName" value="empty_headers"/>
        <parameter name="rowCount" value="0"/>
    </call-method>
            
    <call-method file="Import.xmf">
        <parameter name="importType" value="empty_headers"/>
        <parameter name="sheetName" value="empty_headers"/>
        <parameter name="rowCount" value="0"/>
    </call-method>        
</release-test>
